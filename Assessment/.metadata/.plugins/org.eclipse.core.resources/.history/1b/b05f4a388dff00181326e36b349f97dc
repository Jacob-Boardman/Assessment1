package com.qa.Assessment;

public class Treasure 
{
	private int setLocation;
	private int startingLocation;
	
	Treasure(){}
	
	Treasure(int setLocation)
	{
		if(setLocation > 5)
		{
			startingLocation = setLocation;
		}
		else if (setLocation >=3 && setLocation < 5)
		{
			startingLocation = -setLocation;
		}
	}
}
